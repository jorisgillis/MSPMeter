package io;

import javax.swing.JFrame;


/**
 * Class which exports in tab-delimited format. 
 * @author Joris Gillis
 */
public class TabExporter extends DelimitedExporter {
	
	/**
	 * Constructs a new tab-delimited exporter
	 * @param parent	GUI parent
	 */
	public TabExporter( JFrame parent ) {
		super(parent, "\t");
	}
}
